@prefix gutp: <https://www.gutp.jp/bim-wg#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rec: <https://w3id.org/rec#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

owl:ThingShape a sh:NodeShape ;
    sh:property <http://www.w3.org/2002/07/owl#ThingShape#name> ;
    sh:targetClass owl:Thing .

rec:ArchitectureShape a sh:NodeShape ;
    sh:property <https://w3id.org/rec#ArchitectureShape#adjacentElement>,
        <https://w3id.org/rec#ArchitectureShape#architectedBy>,
        <https://w3id.org/rec#ArchitectureShape#area>,
        <https://w3id.org/rec#ArchitectureShape#capacity>,
        <https://w3id.org/rec#ArchitectureShape#constructedBy>,
        <https://w3id.org/rec#ArchitectureShape#containsElement>,
        <https://w3id.org/rec#ArchitectureShape#documentation>,
        <https://w3id.org/rec#ArchitectureShape#hasPoint>,
        <https://w3id.org/rec#ArchitectureShape#intersectingElement>,
        <https://w3id.org/rec#ArchitectureShape#isFedBySubstance>,
        <https://w3id.org/rec#ArchitectureShape#operatedBy>,
        <https://w3id.org/rec#ArchitectureShape#ownedBy> ;
    sh:targetClass rec:Architecture .

rec:AssetShape a sh:NodeShape ;
    sh:property <https://w3id.org/rec#AssetShape#isPartOf>,
        <https://w3id.org/rec#AssetShape#locatedIn> ;
    sh:targetClass rec:Asset .

rec:BuildingShape a sh:NodeShape ;
    sh:property <https://w3id.org/rec#BuildingShape#address> ;
    sh:targetClass rec:Building .

rec:LevelShape a sh:NodeShape ;
    sh:property <https://w3id.org/rec#LevelShape#levelNumber> ;
    sh:targetClass rec:Level .

rec:SpaceShape a sh:NodeShape ;
    sh:property <https://w3id.org/rec#SpaceShape#hasPart>,
        <https://w3id.org/rec#SpaceShape#isPartOf> ;
    sh:targetClass rec:Space .

