{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": [
    "baseIRI",
    "versionIRI",
    "version",
    "prefixes",
    "languages"
  ],
  "properties": {
    "baseIRI": {
      "type": "string"
    },
    "versionIRI": {
      "type": "string"
    },
    "version": {
      "type": "string"
    },
    "languages": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "prefixes": {
      "type": "object",
      "additionalProperties": {
        "type": "string"
      }
    }
  }
}